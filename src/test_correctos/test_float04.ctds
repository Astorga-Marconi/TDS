/* funcion que calcula la x ^ n , x elevado a la n  */

float potencia(float x, int n){

	int i; 
	boolean neg;
	float prod;
        neg = false;
        i = 1;
        prod = 1.0;

	if (n < 0){
		n = -n;
		neg = true;
	}

	while(i < n || i == n){
		prod = prod * x * 2.0; 
		i = i + 1;
	}
	if (!(!neg)){ //neg true
		return 1.0/prod;
        }
	else{
		return prod;
        }
	return -100.1;
}

float  multRepeat(float b, float a){

  
  float  c;
  c =  a * a * b * b * b * a;
 
  return c;  
  
}

